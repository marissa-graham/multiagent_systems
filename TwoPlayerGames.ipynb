        "        self.representative = None\n",
        "        \n",
        "        self.is_symmetric = (p1 == p2)\n",
        "        self.equivalents = set()\n",
        "        self.indices = []\n",
        "        self.get_equivalents()\n",
        "    \n",
        "    def __eq__(self, other):\n",
        "        \n",
        "        if len(self.equivalents) == len(other.equivalents):\n",
        "            for pref_pair in self.equivalents:\n",
        "                if pref_pair not in other.equivalents:\n",
        "                    return False\n",
        "        return False\n",
        "        \n",
        "    def get_equivalents(self):\n",
        "        \"\"\"\n",
        "        Get the permutation tuples of the preference order rankings for\n",
        "        each player for the games considered equivalent to self.\"\"\"\n",
        "        \n",
        "        for (p,q) in [(self.p1,self.p2),(self.p2,self.p1)]:\n",
        "            switchrows = ((p[2],p[3],p[0],p[1]),(q[1],q[0],q[3],q[2]))\n",
        "            switchcolumns = ((p[1],p[0],p[3],p[2]),(q[2],q[3],q[0],q[1]))\n",
        "            self.equivalents.add(((p[0],p[1],p[2],p[3]),(q[0],q[1],q[2],q[3])))\n",
        "            self.equivalents.add(switchrows)\n",
        "            self.equivalents.add(switchcolumns)\n",
        "        \n",
